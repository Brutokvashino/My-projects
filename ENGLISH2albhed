txt = input("Please type what you want to say in Al-Bhed using all capital letters.")
aa = txt.replace("A", "y") 
ab = aa.replace("B", "p") 
ac = ab.replace("C", "l") 
ad = ac.replace("D", "t") 
ae = ad.replace("E", "a") 
af = ae.replace("F", "v") 
ag = af.replace("G", "k") 
ah = ag.replace("H", "r") 
ai = ah.replace("I", "e") 
aj = ai.replace("J", "z") 
ak = aj.replace("K", "g") 
al = ak.replace("L", "m") 
am = al.replace("M", "s") 
an = am.replace("N", "h") 
ao = an.replace("O", "u") 
ap = ao.replace("P", "b") 
aq = ap.replace("Q", "x") 
ar = aq.replace("R", "n") 
ss = ar.replace("S", "c") 
at = ss.replace("T", "d") 
au = at.replace("U", "i") 
av = au.replace("V", "j") 
aw = av.replace("W", "f") 
ax = aw.replace("X", "q") 
ay = ax.replace("Y", "o") 
az = ay.replace("Z", "w") 
print (az)
